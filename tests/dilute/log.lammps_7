units               real
dimension           3
newton              off
#processors          * * 2
if "$n <= 5" then "boundary p p f" else "boundary p p p"
boundary p p p
atom_style          full

##force field
pair_style          lj/cut/coul/long 4.0
bond_style          harmonic
angle_style         harmonic
kspace_style        pppm 1.0e-6
if "$n <= 5" then "kspace_modify slab 3.0"

##atom definition
